# Backend URL (Production ortamında otomatik ayarlanır)
REACT_APP_BACKEND_URL=https://your-app.emergentagent.com

# WebSocket Port (Production için)
WDS_SOCKET_PORT=443

# Health Check
ENABLE_HEALTH_CHECK=false

# Firebase Cloud Messaging VAPID Keys
# Ekosistem Chat (Layla) projesi için
REACT_APP_VAPID_KEY_LAYLA=your-layla-vapid-key-here

# Biyom (sohbet-cfe7f) projesi için
REACT_APP_VAPID_KEY_BIYOM=your-biyom-vapid-key-here
